---
import MarkdownWrapper from '../components/MarkdownWrapper.astro';
import DownloadSection from '../components/react/DownloadSection';
import Home from '../layouts/Home.astro';

import * as markdown from '../content/download.md';
const { Content, frontmatter } = markdown;
---

<Home title={frontmatter.title} description={frontmatter.description}>
  <MarkdownWrapper><Content /></MarkdownWrapper>
  <DownloadSection
    repository='https://github.com/whatphilipcodes/whatphilipdoes'
    client:load
  />
</Home>
